{
  "name": "klickhaus",
  "version": "1.0.0",
  "description": "ClickHouse CDN Analytics Dashboard",
  "type": "module",
  "scripts": {
    "start": "live-server --port=5391 --open=/ --ignore=scripts,.github,.claude,hars,node_modules --ignorePattern=\"\\\\.md$|package.*\\\\.json$|screenshot\\.png$\"",
    "test": "node --test --experimental-test-coverage --test-coverage-lines=88.4",
    "lint": "eslint .",
    "dead-code": "knip",
    "cpd": "jscpd js/ scripts/",
    "cpd:ci": "jscpd js/ scripts/ --reporters console",
    "prepare": "husky"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "^3.0.16",
    "canvas": "3.2.1",
    "husky": "^9.1.7",
    "jscpd": "^4.0.5",
    "knip": "^5.82.1",
    "lint-staged": "^16.2.7",
    "live-server": "^1.2.2"
  },
  "keywords": [
    "clickhouse",
    "cdn",
    "analytics",
    "dashboard"
  ],
  "license": "Apache-2.0",
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.mjs": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "puppeteer-core": "24.36.1"
  }
}
