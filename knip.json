{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "js/main.js",
    "sw.js",
    "scripts/*.mjs",
    "eslint.config.js",
    "web-test-runner.config.mjs"
  ],
  "project": [
    "js/**/*.js",
    "scripts/**/*.mjs"
  ],
  "ignore": [
    "js/**/*.test.js"
  ],
  "ignoreDependencies": [
    "live-server",
    "chai",
    "@playwright/cli"
  ],
  "ignoreBinaries": [
    "eslint",
    "playwright"
  ]
}
